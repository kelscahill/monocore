{% if logo.image.src %}
  <div class="o-logo{{ logo.classes ? ' ' ~ logo.classes }}">
    {% if logo.link %}
      <a href="{{ logo.link }}" class="o-logo__link">
    {% endif %}
      <img class="o-logo__image" src="{{ logo.image.src }}" alt="{{ logo.image.alt ? logo.image.alt : "Logo" }}"{{ logo.image.width ? ' width="' ~ logo.image.width ~ '"' }}{{ logo.image.height ? ' height="' ~ logo.image.height ~ '"' }} />
    {% if logo.link %}
      </a>
    {% endif %}
  </div>
{% endif %}
